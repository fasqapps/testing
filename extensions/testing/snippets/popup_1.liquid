                  <div class="landingCountry" value="{{ localization.country.iso_code }}"></div>
                  <button
                    class="dropdown-btn saCountryList saSelect"
                    id="saCountryButton"
                    type="button"
                    tabindex="-1"
                  >
                    {{ localization.country.name }}
